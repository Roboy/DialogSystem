{
  "initialState": "Greetings",
  "states": [
    {
      "identifier": "Greetings",
      "implementation": "roboy.dialog.states.multiPartyStates.PassivGreetingsStateParty",
      "transitions": {
        "greetingDetected": "Intro"
      }
    },
    {
      "identifier": "Intro",
      "implementation": "roboy.dialog.states.multiPartyStates.IntroductionStateParty",
      "transitions": {
        "knownPerson": "PersonalQA",
        "newPerson": "PersonalQA"
      },
      "parameters": {
        "infoFile": "resources/sentences/RoboyInfoList.json"
      }
    },
    {
      "identifier": "PersonalQA",
      "implementation": "roboy.dialog.states.multiPartyStates.PersonalInformationAskingStateParty",
      "transitions": {
        "questionAnswering": "QuestionAnswering"
      },
      "parameters": {
        "qaFile": "resources/sentences/QAList.json"
      }
    },
    {
      "identifier": "QuestionAnswering",
      "implementation": "roboy.dialog.states.multiPartyStates.QuestionAnsweringStateParty",
      "fallback": "WildTalk",
      "transitions": {
        "finishedQuestionAnswering": "Farewell",
        "loopToNewPerson": "PersonalQA",
        "loopToKnownPerson": "PersonalQA",
        "switchToGaming": "PersonalQA"
      }
    },
    {
      "identifier": "WildTalk",
      "implementation": "roboy.dialog.states.multiPartyStates.WildTalkStateParty",
      "transitions": {}
    },
    {
      "identifier": "Farewell",
      "implementation": "roboy.dialog.states.multiPartyStates.FarewellStateParty",
      "transitions": {}
    }
  ]
}